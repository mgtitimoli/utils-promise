{
  "name": "@mgtitimoli/utils-promise",
  "description": "Promise Utilities",
  "version": "0.0.0",
  "author": "Mauro Gabriel Titimoli <mgtitimoli@gmail.com> (https://github.com/mgtitimoli)",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/mgtitimoli/utils-promise#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgtitimoli/utils-promise.git"
  },
  "bugs": {
    "url": "https://github.com/mgtitimoli/utils-promise/issues"
  },
  "scripts": {
    "build": "run-s build:clean build:code build:readme",
    "build:clean": "rm -rf dist",
    "build:code": "ttsc --inlineSources --sourceMap --sourceRoot '.'",
    "build:readme": "run-s build:readme:from-package build:readme:doctoc",
    "build:readme:doctoc": "doctoc README.md",
    "build:readme:from-package": "pkg-to-readme --force --template readmeTemplate.ejs",
    "debug": "node --require ts-node/register --require tsconfig-paths/register --inspect src/index.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/prompt-cli": "^9.1.2",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concat-md": "^0.3.5",
    "doctoc": "^1.4.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.11",
    "np": "^6.4.0",
    "npm-run-all": "^4.1.5",
    "pkg-to-readme": "^1.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typedoc": "^0.18.0",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-map": "^1.2.1",
    "typedoc-plugin-internal-external": "^2.2.0",
    "typedoc-plugin-markdown": "^2.4.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@mgtitimoli/utils-error": "^1.0.2",
    "tslib": "^2.0.1"
  }
